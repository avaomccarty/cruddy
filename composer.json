{
    "name": "ottiem/cruddy",
    "description": "A powerful but simple CRUD builder for Laravel.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ava McCarty",
            "email": "avaomccarty@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Cruddy\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^8.0.0",
        "illuminate/console": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/database": "^8.0",
        "symfony/console": "^5.0",
        "orchestra/testbench": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cruddy\\ServiceProvider"
            ]
        }
    }
}
